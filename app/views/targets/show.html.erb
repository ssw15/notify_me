<div class="page-header">
  <%= link_to targets_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_target_path(@target), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show target</h1>
</div>

<dl class="dl-horizontal">
  <dt>Page url:</dt>
  <dd><%= @target.page_url %></dd>

  <dt>Element selector:</dt>
  <dd><%= @target.element_selector %></dd>

  <dt>Current content:</dt>
  <dd><%= @content %></dd>

</dl>





